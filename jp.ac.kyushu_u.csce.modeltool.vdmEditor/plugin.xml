<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="%editor.name"
            icon="icons/etool/vdm.gif"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            class="jp.ac.kyushu_u.csce.modeltool.vdmEditor.editor.VDMEditor"
            id="jp.ac.kyushu_u.csce.modeltool.vdm.vdmeditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            description="%context.description"
            id="jp.ac.kyushu_u.csce.modeltool.vdm.editorScope"
            name="%context.name"
            parentId="org.eclipse.ui.textEditorScope">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            description="%category.description"
            id="jp.ac.kyushu_u.csce.modeltool.vdm.editor.category"
            name="%category.name">
      </category>
      <command
            categoryId="jp.ac.kyushu_u.csce.modeltool.vdm.editor.category"
            defaultHandler="jp.ac.kyushu_u.csce.modeltool.vdmEditor.handler.DictionarySearchHandler"
            id="jp.ac.kyushu_u.csce.modeltool.vdm.dictionarysearch"
            name="%command.name">
      </command>
      <command
            categoryId="jp.ac.kyushu_u.csce.modeltool.vdm.editor.category"
            defaultHandler="jp.ac.kyushu_u.csce.modeltool.vdmEditor.handler.VDMSearchHandler"
            id="jp.ac.kyushu_u.csce.modeltool.vdm.vdmsearch"
            name="%command.name.0">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="jp.ac.kyushu_u.csce.modeltool.vdm.dictionarysearch"
            contextId="jp.ac.kyushu_u.csce.modeltool.vdm.editorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+D">
      </key>
      <key
            commandId="jp.ac.kyushu_u.csce.modeltool.vdm.vdmsearch"
            contextId="jp.ac.kyushu_u.csce.modeltool.vdm.editorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+F">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="jp.ac.kyushu_u.csce.modeltool.vdm.toolbar">
            <command
                  commandId="jp.ac.kyushu_u.csce.modeltool.vdm.vdmsearch"
                  icon="icons/etool/search_vdm.gif"
                  style="push">
               <visibleWhen>
                  <reference
                        definitionId="jp.ac.kyushu_u.csce.modeltool.vdm.vdmeditor.isActiveEditor">
                  </reference>
               </visibleWhen>
            </command>
            <command
                  commandId="jp.ac.kyushu_u.csce.modeltool.vdm.dictionarysearch"
                  icon="icons/etool/search_dic.gif"
                  style="push">
               <visibleWhen>
                  <reference
                        definitionId="jp.ac.kyushu_u.csce.modeltool.vdm.vdmeditor.isActiveEditor">
                  </reference>
               </visibleWhen>
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="jp.ac.kyushu_u.csce.modeltool.explorer.explorerExtension">
      <fileExtension
            editorId="jp.ac.kyushu_u.csce.modeltool.vdm.vdmeditor"
            extensions="vpp, vdmpp">
      </fileExtension>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="jp.ac.kyushu_u.csce.modeltool.vdm.vdmeditor.isActiveEditor">
         <or>
            <with
                  variable="activeEditorId">
               <equals
                     value="jp.ac.kyushu_u.csce.modeltool.vdm.vdmeditor">
               </equals>
            </with>
         </or>
      </definition>
   </extension>

</plugin>
